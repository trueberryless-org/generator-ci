{
  "name": "@trueberryless-org/generator-starlight-plugin",
  "version": "0.1.12",
  "description": "Yeoman generator for continuous integration files for trueberryless-org",
  "license": "MIT",
  "type": "module",
  "homepage": "https://generator-starlight-plugin.trueberryless.org",
  "author": {
    "name": "trueberryless",
    "email": "trueberryless+github@gmail.com",
    "url": "https://trueberryless.org"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.ts",
  "keywords": [
    "continuous integration",
    "trueberryless-org",
    "yeoman-generator",
    "github-actions"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^29.7.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "camelcase": "^8.0.0",
    "chalk": "^2.1.0",
    "latest-version": "^9.0.0",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "packageManager": "pnpm@9.3.0",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trueberryless-org/generator-starlight-plugin.git"
  },
  "bugs": "https://github.com/trueberryless-org/generator-starlight-plugin/issues"
}
